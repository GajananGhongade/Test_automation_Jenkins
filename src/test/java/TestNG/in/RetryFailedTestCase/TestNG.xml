<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Executing the Failed Test cases">
    <!--    listener-->
    <listeners>
        <listener class-name="TestNG.in.RetryFailedTestCase.RetryListener"/>
    </listeners>

    <test name="Failed Test cases">
        <parameter name="browser" value="chrome"/>
        <classes>
            <class name="TestNG.in.GroupTestCases"/>
        </classes>
    </test>
</suite>







